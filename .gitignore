# Ignore directories generated by Composer
/vendor/
/web/sites/all/drush/
/web/sites/all/modules/contrib/
/web/sites/all/themes/contrib/
/web/sites/all/libraries/
/lib/profiles/*/modules/contrib/
/lib/profiles/*/themes/contrib/
/lib/profiles/*/libraries/

# Ignore Drupal 7 core files.
web
/web/*
!/web/*/
/web/includes/
/web/misc/
/web/modules/
/web/profiles/README.txt
/web/profiles/minimal/
/web/profiles/standard/
/web/profiles/testing/
/web/scripts/
/web/sites/README.txt
/web/sites/all/*/README.txt
/web/sites/example.sites.php
/web/themes/

# Ignore configuration files that may contain sensitive information.
/web/sites/*/*.settings.php
/web/sites/*/settings*.php

# Ignore local configuration files
grumphp.yml
runner.yml

# Ignore some theme files.
lib/profiles/ipr/themes/ipr_theme/.sass-cache

# Ignore paths that contain user-generated content.
/web/sites/*/files
/web/sites/*/private

# Ignore paths that contain third-party components.
/lib/profiles/*/themes/custom/ipr_theme/bootstrap

# Ignore files generated by PhpStorm
/.idea/
